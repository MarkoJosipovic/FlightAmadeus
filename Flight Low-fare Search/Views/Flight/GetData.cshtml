        <td>
            @Html.DisplayFor(modelItem => item.departure) ->
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.datumPolaska)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.polazakPresjedanje) ->
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.polazakPresjedanjeDatum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dolazakPresjedanje) ->
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dolazakPresjedanjeDatum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.arrival) ->
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.datumDolaska)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.brojPresjedanja)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.brojSlobodnihMjesta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cijena)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.valuta)
        </td>

    </tr>
}

</table>
