@model IEnumerable<Flight_Low_fare_Search.Models.InfoLet>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.departure)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.datumPolaska)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.polazakPresjedanje)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.polazakPresjedanjeDatum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dolazakPresjedanje)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dolazakPresjedanjeDatum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.arrival)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.datumDolaska)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.brojPresjedanja)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.brojSlobodnihMjesta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cijena)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.valuta)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
